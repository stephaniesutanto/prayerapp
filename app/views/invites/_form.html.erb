<%= form_for [@group, @invite] do |f| %>
	<div class="form-group">
		  <div class="col-xs-3">
			<%= f.label :email %>
			<%= f.email_field :email, class: 'form-control ' %>
			</br>
			<%= f.submit 'Send', class: 'btn btn-primary' %>
			</br>
			<%= link_to 'Back', my_groups_groups_path %>
		  </div>
	</div>
<% end %>
